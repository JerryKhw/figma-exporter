{
  "name": "@jerry/figma-exporter",
  "version": "3.1.0",
  "description": "Figma Exporter",
  "type": "module",
  "scripts": {
    "test": "pnpm run tsc && pnpm run build",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx}\"",
    "lint": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "lint:check": "eslint \"src/**/*.{ts,tsx}\"",
    "tsc": "pnpm run tsc:main && pnpm run tsc:ui",
    "tsc:main": "tsc --noEmit -p src/plugin",
    "tsc:ui": "tsc --noEmit -p src/ui",
    "tsc:watch": "concurrently -n main,ui \"pnpm:tsc:main -- --watch --preserveWatchOutput\" \"pnpm:tsc:ui -- --watch --preserveWatchOutput\"",
    "build": "pnpm run build:ui && pnpm run build:main",
    "build:main": "node esbuild.js",
    "build:ui": "vite build --emptyOutDir=false",
    "build:watch": "concurrently -n main,ui \"pnpm:build:main -- --watch\" \"pnpm:build:ui -- --watch\"",
    "dev": "concurrently -n main,ui \"pnpm:dev:main\" \"pnpm:dev:ui\"",
    "dev:main": "concurrently -n tsc,build \"pnpm:tsc:main -- --watch --preserveWatchOutput\" \"pnpm:build:main -- --watch\"",
    "dev:ui": "vite build --watch"
  },
  "author": {
    "name": "JERRY/JIHOON KIM",
    "email": "jerry@jerrykhw.dev",
    "url": "https://github.com/JerryKhw"
  },
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@figma/plugin-typings": "^1.116.0",
    "@preact/preset-vite": "^2.10.2",
    "@radix-ui/react-select": "2.1.4",
    "@radix-ui/react-slot": "1.1.1",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/node": "^22.14.0",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.7.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "concurrently": "^9.2.0",
    "esbuild": "^0.25.8",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "fflate": "^0.8.2",
    "globals": "^16.3.0",
    "lucide-react": "^0.475.0",
    "postcss": "^8.5",
    "preact": "^10.26.9",
    "prettier": "^3.6.2",
    "react": "npm:@preact/compat@^18.3.1",
    "react-dom": "npm:@preact/compat@^18.3.1",
    "superstruct": "^2.0.2",
    "tailwind-merge": "^2.5.5",
    "tailwindcss": "^4.1.11",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.38.0",
    "vite": "^7.0.6",
    "vite-plugin-singlefile": "^2.3.0",
    "vite-plugin-singlefile-compression": "^2.0.5",
    "webp-converter-browser": "^1.0.4",
    "zustand": "^5.0.6",
    "browser-image-compression": "^2.0.2"
  },
  "packageManager": "pnpm@10.13.1+sha512.37ebf1a5c7a30d5fabe0c5df44ee8da4c965ca0c5af3dbab28c3a1681b70a256218d05c81c9c0dcf767ef6b8551eb5b960042b9ed4300c59242336377e01cfad"
}
